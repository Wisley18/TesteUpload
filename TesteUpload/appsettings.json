{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UploadFiles": {
    "Formats": "image/png,image/jpeg,image/jpg,application/pdf,text/plain,application/vnd.openxmlformats-officedocument.wordprocessingml.document,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
    "MaxLength": "10000000",
    "ImageFolder": "/images/{0}",
    "DocumentFolder": "/docs/{0}",
    "StaticCurrentDomain": "https://localhost:44358"
  }
}
